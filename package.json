{
  "name": "@nota/rxjs-operators",
  "version": "0.2.2",
  "description": "A collection of operators for rxjs used by @nota",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm-run-all lint build:tsc",
    "build:tsc": "tsc -p ./tsconfig.json",
    "clean": "npm-run-all clean:docs clean:temp clean:dist",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:temp": "rimraf .tmp",
    "format": "prettier --write --config .prettierrc.json 'src/**/*.ts'",
    "format:check": "prettier-check --config .prettierrc.json 'src/**/*.ts'",
    "lint": "npm-run-all tslint format:check",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register src/**/*.spec.ts",
    "test:watch": "TS_NODE_FILES=true mocha -r ts-node/register -w --watch-extensions 'ts' src/**/*.spec.ts",
    "prepare": "npm run build",
    "tslint": "tslint -p ./tsconfig.json -c ./tslint.json"
  },
  "author": {
    "name": "nota",
    "url": "http://nota.dk"
  },
  "contributors": [
    {
      "name": "Daniel Dam Freiling",
      "email": "dfg@nota.dk"
    },
    {
      "name": "Morten Anton Bach Sj√∏gren",
      "url": "http://mabs.dk",
      "email": "m_abs@mabs.dk"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "rxjs": "^6.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.1",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "rxjs-marbles": "^4.3.2",
    "rxjs-tslint": "^0.1.5",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-no-circular-imports": "^0.6.1",
    "typescript": "^3.1.6"
  }
}
