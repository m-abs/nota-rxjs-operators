{
  "name": "@nota/rxjs-operators",
  "version": "0.2.2",
  "description": "A collection of operators for rxjs used by @nota",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm-run-all lint build:tsc",
    "build:tsc": "tsc -p ./tsconfig.json",
    "clean": "npm-run-all clean:docs clean:temp clean:dist",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:temp": "rimraf .tmp",
    "format": "prettier --write --config .prettierrc.json 'src/**/*.ts'",
    "format:check": "prettier-check --config .prettierrc.json 'src/**/*.ts'",
    "lint": "npm-run-all tslint format:check",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register src/**/*.spec.ts",
    "test:watch": "TS_NODE_FILES=true mocha -r ts-node/register -w --watch-extensions 'ts' src/**/*.spec.ts",
    "prepare": "npm run build",
    "tslint": "tslint -p ./tsconfig.json -c ./tslint.json"
  },
  "author": {
    "name": "nota",
    "url": "http://nota.dk"
  },
  "contributors": [
    {
      "name": "Daniel Dam Freiling",
      "email": "dfg@nota.dk"
    },
    {
      "name": "Morten Anton Bach Sj√∏gren",
      "url": "http://mabs.dk",
      "email": "m_abs@mabs.dk"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/m-abs/nota-rxjs-operators.git"
  },
  "homepage": "https://github.com/m-abs/nota-rxjs-operators",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.1",
    "rxjs-marbles": "^5.0.2",
    "rxjs-tslint": "^0.1.7",
    "sinon": "^7.3.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "tslint-no-circular-imports": "^0.6.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "rxjs": "^6.5.1",
    "tslib": "^1.9.3",
    "fast-deep-equal": "^2.0.1"
  }
}
